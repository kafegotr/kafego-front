{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
       "no-console": "off",
       "eslint-disable-next-line": "off",
       "import/prefer-default-export": "off",
       "no-unused-vars": "off",
       "no-undef": "off",
       "no-return-await": "off",
       "camelcase": "off",
       "import/extension": "off",
       "react-hooks/rules-of-hooks": "off",
       "no-unused-expressions": "off",
    }
}
